packages:
  yum:
    git: []
    postgresql93-devel: []
    libjpeg-turbo-devel: []
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "GlobalNorth.settings"
    PYTHONPATH: "/var/app/current:$PYTHONPATH"
  aws:elasticbeanstalk:container:python:
    WSGIPath: "GlobalNorth.wsgi:application"
    NumProcesses: 3
    NumThreads: 20
container_commands:
  00_test_output:
    command: "echo 'testing.....'"
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py makemigrations accounts home services Payment && python3 manage.py migrate --noinput"
    leader_only: true
